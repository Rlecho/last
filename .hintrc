{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "listitem": "off"
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-required-children": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off"
      }
    ],
    "meta-viewport": "off",
    "meta-charset-utf-8": "off",
    "compat-api/css": [
      "off",
      {
        "ignore": [
          "-webkit-overflow-scrolling"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 114",
    "not and_ff <= 113",
    "not chrome <= 114",
    "not edge <= 114",
    "not firefox <= 114",
    "not ios_saf <= 16.5",
    "not opera <= 99",
    "not safari <= 16.5",
    "not samsung <= 21"
  ]
}